
#OUTPUT
#X greater than 20.
#X greater than 10.
#X greater than 5.
#END

#RET 0

func printf(ln:str, x:int) is extern
func puts(ln:str) is extern

func cmp(x:int) is
begin
    if x > 20 then
        puts("X greater than 20.");
    elif x > 10 then
        puts("X greater than 10.");
    elif x > 5 then
        puts("X greater than 5.");
    end

    return 0;
end

func main is
begin
    cmp(25);
    cmp(4);
    cmp(15);
    cmp(7);
    cmp(1);
    return 0;
end

