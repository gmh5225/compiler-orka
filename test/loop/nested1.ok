func printf(ln:str, x:int) is extern

func main -> int is
    x : int = 0;
    i : int = 0;
begin
    while x < 10 do
        i = 1;
        while i <= x + 1 do
            printf("*");
            i = i + 1;
        end
        printf("\n");
        
        x = x + 1;
    end
    
    return 0;
end

