#RET 0

func puts(ln : str) is extern

func compare(x:int) is
begin
    if x > 10 then
        puts("Greater than 10");
        if x > 15 then
            puts("Greater than 15");
        else
            puts("Less than 15");
        end
        puts("Done");
    else
        puts("Less");
    end
    
    return;
end

func main is
    x : int = 40;
begin
    compare(x);
    puts("---");
    compare(3);
    puts("---");
    compare(11);
    puts("---");
    
    return 0;
end

