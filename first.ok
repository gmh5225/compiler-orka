import std.io;

struct Lex is
    input : str := "";
    index : int := 0;
end

enum Token is
    None,
    
    Int,
    
    ID,
    IntL,
    
    Assign,
    SemiColon
end

func getNext(lex : Lex) -> int is
    println(lex.input);
    return 0;
end

func main(args:str[]) -> int is
    struct lex : Lex;
    lex.input := "int x = 30;";
    
    getNext(lex);
    return 0;
end
