#RET 0

extern func printf(line:str, x:int);

func printArray(numbers:int[]) is
    x : int = numbers[2];
    size : int = sizeof(numbers);
begin
    printf("%d\n", x);
    printf("Size: %d\n", size);
end

func main -> int is
    numbers : int[10];
begin
    numbers[2] = 242;
    printArray(numbers);
    
    return 0;
end

