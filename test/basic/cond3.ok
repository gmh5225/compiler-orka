
#OUTPUT
#X greater than 20.
#X greater than 10.
#X greater than 5.
#END

#RET 0

extern func printf(ln:str, x:int)
extern func puts(ln:str)

func cmp(x:int)
begin
    if x > 20
        puts("X greater than 20.");
    elif x > 10
        puts("X greater than 10.");
    elif x > 5
        puts("X greater than 5.");
    end

    return 0;
end

func main
begin
    cmp(25);
    cmp(4);
    cmp(15);
    cmp(7);
    cmp(1);
    return 0;
end

